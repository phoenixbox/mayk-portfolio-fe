{
  "name": "mayk-portfolio",
  "private": true,
  "version": "1.0.0",
  "devDependencies": {
    "babelify": "^5.0.5",
    "backdash": "^1.1.2-2.4.1",
    "bluebird": "^2.5.3",
    "bootstrap": "^3.3.1",
    "browserify": "^9.0.0",
    "browserify-istanbul": "^0.2.1",
    "browserify-shim": "^3.8.10",
    "chai": "^2.1.1",
    "classnames": "^2.1.3",
    "confidence": "^1.2.0",
    "envify": "^3.2.0",
    "es5-shim": "^4.0.6",
    "eslint": "^0.24.0",
    "eslint-plugin-react": "^2.6.4",
    "exorcist": "^0.3.0",
    "flux": "^2.0.1",
    "fuzzy": "^0.1.0",
    "gulp": "^3.9.0",
    "gulp-csso": "^1.0.0",
    "gulp-less": "^3.0.3",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^1.5.2",
    "highcharts-release": "^4.1.8",
    "karma": "^0.12.31",
    "karma-browserify": "~4.0.0",
    "karma-chrome-launcher": "^0.1.7",
    "karma-coverage": "^0.2.7",
    "karma-firefox-launcher": "^0.1.4",
    "karma-mocha": "^0.1.10",
    "keymirror": "^0.1.1",
    "mocha": "^2.2.1",
    "nodemon": "^1.3.6",
    "react": "^0.13.3",
    "react-bootstrap": "^0.23.7",
    "react-frame-component": "^0.3.4",
    "react-inlinesvg": "^0.4.2",
    "react-localstorage": "^0.2.4",
    "react-markdown": "^1.0.3",
    "react-router": "^0.13.3",
    "react-select": "^0.5.2",
    "react-slick": "^0.6.1",
    "react-typeahead": "1.0.1",
    "reactify": "^1.1.0",
    "rewireify": "^0.2.1",
    "sinon": "^1.13.0",
    "uglify-js": "^2.4.14",
    "watchify": "~3.1.2"
  },
  "scripts": {
    "dev": "gulp dev",
    "build": "browserify app -d | exorcist -b './app' public/js/mayk.js.map > public/js/mayk.js",
    "build-less": "gulp less",
    "build-production": "npm run build-production-js && npm run build-production-css",
    "build-production-css": "gulp css",
    "build-production-js": "NODE_ENV=production browserify app | uglifyjs -cm > public/js/mayk.min.js",
    "build-test": "NODE_ENV=test browserify test/index.js -t rewireify -t babelify -t envify -d | exorcist -b './app' test/test-bundle.js.map > test/test-bundle.js",
    "start": "node server.js",
    "test": "NODE_ENV=test ./node_modules/karma/bin/karma start --browsers Firefox --single-run",
    "watch-test": "NODE_ENV=test ./node_modules/karma/bin/karma start",
    "watchify": "watchify app -d -v -o 'exorcist -b './app' public/js/mayk.js.map > public/js/mayk.js'",
    "watchify-test": "NODE_ENV=test watchify test/index.js -o 'exorcist -b './app' test/test-bundle.js.map > test/test-bundle.js' -t rewireify -t babelify -t envify -dv"
  },
  "browserify": {
    "transform": [
      "babelify",
      "envify"
    ]
  },
  "dependencies": {
    "bell": "^4.0.0",
    "config": "^1.14.0",
    "good": "^6.1.4",
    "good-console": "^5.0.2",
    "hapi": "^8.6.1",
    "hapi-auth-basic": "^3.0.0",
    "hapi-auth-cookie": "^3.0.1",
    "lodash": "^2.4.1",
    "marked": "^0.3.3",
    "moment": "^2.10.3",
    "superagent": "^1.2.0",
    "swig": "^1.4.2"
  },
  "engines": {
    "node": "0.12.x",
    "npm": "2.7.x"
  },
  "main": "server.js",
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": ""
  }
}
